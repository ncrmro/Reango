{
  "name": "reango",
  "version": "1.0.0",
  "description": "reango",
  "scripts": {
    "start": "./node_modules/.bin/babel-node webpack/server/index.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.js",
    "lint": "eslint --ignore-path .gitignore src && prospector ./src/apps --uses django --ignore-patterns */migrations",
    "clean": "rm -rf ./static && mkdir ./static && mkdir ./static/bundles",
    "postinstall": "python3 ./manage.py graphql_schema && npm run build"
  },
  "pre-commit": "lint",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ncrmro/reango.git"
  },
  "engines": {
    "node": "6.7.0",
    "npm": "3.10.3"
  },
  "author": "Nicholas Romero",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "chalk": "^1.1.3",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "pre-commit": "^1.1.3",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.3",
    "webpack-dev-server": "2.4.2"
  },
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-relay-plugin": "^0.9.3",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "lodash": "^4.17.2",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.2.0",
    "precss": "^1.4.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
